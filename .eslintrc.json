{
  "env": {
    "browser": true,
    "node": true,
    "es2022": true
  },
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": ["error", "ignorePackages"],
    "import/no-unresolved": "off",
    "import/no-absolute-path": "off"
  }
}
